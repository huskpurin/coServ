ctrl.startup = function() {	
	hover();
	var uri = "<%=bi.uri%>";
	var pathname = location.pathname.split('?')[0];
	ctrl.sel('#menu').find('a').each(function(item) {
		console.log($(item));
		var href = $(item).attr('href');
		console.log(href);
		var path = href.split('?')[0];
		if (path == pathname) {
			$( item ).find('.arrow').show();
		}

	});
}
function hover() {
	$( ".bu" ).hover(function() {		
		$( this ).find('.arrow').show();					
	},function() {
		$( this ).find('.arrow').hide();
	});
}

