<a href="/back/attendee/list/<%=bi.id%>">
	<span class="btn btn-md btn-default pull-left">
		<i class="glyphicon glyphicon-arrow-left"></i>回上頁
	</span>
</a>
<table class="table">
	<thead>
	  <tr>
	    <th>姓名</th>
	    <th>性別</th>
	    <th>公司名稱</th>
	    <th>公司設立年份</th>
	    <th>產業類別</th>
	    <th>營業項目</th>
			<th>職稱</th>
			<th>聯絡電話</th>
			<th>傳真</th>
			<th>E-mail</th>
			<th>聯絡地址</th>
			<th>報到碼</th>
	  </tr>
	</thead>
	<tbody>
		<%
			if (value && value.list.length > 1) {
				value.list.forEach(function(item) {
		%>
	  <tr>
	    <td><%=item.fname%></td>
	    <td><%=item.gender%></td>
			<td><%=item.orgName%></td>
			<% if (item.json && item.json !== null) {%>
			<td><%=item.json.orgYear%></td>
			<td><%=item.json.orgType%></td>
			<td><%=item.json.product%></td>
			<% } else { %>
			<td>---</td>
			<td>---</td>
			<td>---</td>
			<%}%>
			<td><%=item.jobTitle%></td>
			<td><%=item.phone%></td>
			<td><%=item.fax%></td>
			<td><%=item.email%></td>
			<td><%=item.addr%></td>
			<td><%=item.regCode%></td>
	  </tr>
		<% });
		} %>
	</tbody>
</table>
