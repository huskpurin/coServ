<% value.list.forEach(function(item) { %>
  <div style="border-bottom: 1px solid #333">
    <p>ngID: <%=item.ngID%></p>
    <p>isPublic: <%=item.isPublic%></p>
    <p>title: <%=item.title%></p>
    <p>summary: <%=item.summary%></p>
    <p>createTime: <%=new Date(item.createTime)%></p>
    <p>mdTime: <%=new Date(item.mdTime)%></p>
    <p>dspName: <%=item.dspName%></p>
    <a href="/back/article/info/<%=item.ngID%>">sss</a>
    <button class="btn btn-green btn-sm wc40 paLR5" data-toggle="modal" data-target="#editorModal" onclick="<%=ctrl%>.embedEditor('<%=item.ngID%>');">編輯</button>
  </div>
<% }); %>

<!-- 編輯器開始 -->
<!-- data-backdrop="static" -->
<div class="modal fade modalstyle" id="editorModal" role="dialog" aria-labelledby="editorModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="editor modal-content">
      <!-- load Editor-->
    </div>
  </div>
</div>
<!-- 編輯器結束 -->
