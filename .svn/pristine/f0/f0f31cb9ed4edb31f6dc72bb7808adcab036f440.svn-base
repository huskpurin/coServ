@import "wonderful.scss";
@import "scrollbar.scss";

@mixin inner{
	@include float;
	-webkit-padding-start: 25px;
	li {	
	line-height: 24px;
	font-size: $fs1;
	}
}

.wc30 {
	width: $w *0.27;
	@include inner;	
}
.wc33 {
	width: $w *0.32;
	@include inner;
}
.va {
	width: $w *0.43;
	@include inner;
	vertical-align: bottom;
	margin-bottom: 0px;
	margin-left: -20px;
	margin-top: 170px;
}
.clear {
	@include clear;
}