<!-- Contact Form -->
<div class="modal fade modalstyle" id="contact" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">

       <!-- modal-header -->
	    <div class="modal-header">
		    <button type="button" class="close alpha80" data-dismiss="modal">
		    	<span aria-hidden="true">&times;</span>  
		    </button>
		    <div class="modal-title"><h3><%= ph.title %></h3></div>
		    <hr class="hr_2">
		</div>
		<!-- /.modal-header -->		
		<div class="modal-body col-lg-12 col-md-12 col-sm-12 col-xs-12">
		    <form class="form-horizontal" role="form">
				<div class="form-group">					
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
						<input type="text" class="form-control" id="name" name="name" placeholder="<%= ph.name %>" required>
					</div>
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
				        <input type="text" class="form-control" id="mail" name="mail" placeholder="<%= ph.mail %>" required>
					</div>
				</div>
				<div class="form-group">
					<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
						<input type="text" class="form-control" id="sub" name="sub" placeholder="<%= ph.sub %>" required>
					</div>
				</div>
				<div class="form-group">
					<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
						<textarea type="text" class="form-control" id="text" name="massage" rows="6" placeholder="<%= ph.massage %>" required></textarea>
					</div>
				</div>
			</form>
		</div>
		<!-- /.modal-body -->
		<div class="modal-footer">	
			<a href="#" class="btn btn-send" onclick="_wf.getCtrl('c9').send();">
				<i class="glyphicon glyphicon-send"></i></a>
		</div>
		<!-- /.modal-footer -->

    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<script>
$(document).ready(function() {
	// var i=$(".bu").index();
	$( ".bu" ).hover(function() {		
		$("li.active").find('.arrow').hide();
		$("li.active").css('border-bottom-color', 'rgba(255,255,255, 0)');
		$( this ).find('.arrow').show();
		/*if(i=0){
			$("li.active").css('border-bottom-color', '#599CCE');
		},else if(i>0){
			$("li.active").css('border-bottom-color', 'rgba(255,255,255, 0)');
		}*/						
	},function() {
		$( this ).find('.arrow').hide();
		$("li.active").find('.arrow').show();
		$("li.active").css('border-bottom-color', '#599CCE');
	});
	$( ".active" ).hover(function() {		
		$( this ).find('.arrow').show();
		$( this ).css('border-bottom-color', '#599CCE');	
	},function() {
		$( this ).find('.arrow').hide();
		$( this ).css('border-bottom-color', 'rgba(255,255,255, 0)');
	});
});
</script>