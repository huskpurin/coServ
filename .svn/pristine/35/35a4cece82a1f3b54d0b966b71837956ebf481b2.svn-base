<h3>後台登入</h3>
<form id="login" role="form">
  <div class="form-group col-md-6">
    <!-- <label for="accName" class="control-label">帳號</label> -->
    <input type="text" id="accName" name="accName" class="form-control text-center" value="" placeholder="帳號">
  </div>

  <div class="form-group col-md-6">
    <!-- <label for="passwd" class="control-label">密碼</label> -->
    <input type="password" id="passwd" name="passwd" class="form-control text-center" value="" placeholder="密碼">
  </div>
  <button type="button" class="form-control" onclick="<%=ctrl%>.doLogin();">LOGIN</button>
</form>
