<% var siID = bi.id %>

<div class="container-fluid">
  <div class="row">
    <div class="col-md-12 main">
      <h1 class="page-header">Dashboard</h1>
      <span class="btn btn-md btn-default">
        <a href="/back/session/list"><i class="glyphicon glyphicon-arrow-left"></i>場次管理</a>
      </span>
      <h2 class="sub-header">
        報名管理
      </h2>
      <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th></th>
              <th>姓名</th>
              <th>性別</th>
              <th>公司名稱</th>
              <th>電話</th>
              <th>傳真</th>
              <th>E-mail</th>
              <th>報到碼</th>
            </tr>
          </thead>
          <tbody>
            <% var i = 1;
               if (value && value.list.length > 0) {
                 value.list.reverse().forEach(function(item) {
            %>
            <!-- open = ( onTime < now && now < offTime) ? true : false; -->
            <tr>
              <td><%= i++ %></td>
              <td><%= item.fname %></td>
              <td><%= item.gender %></td>
              <td><%= item.orgName %></td>
              <td><%= item.phone %></td>
              <td><%= item.fax %></td>
              <td><%= item.email %></td>
              <td><%= item.regCode %></td>
              <td>
                <button class="btn btn-danger" onclick="<%=ctrl%>.cancel('<%=item.siID%>','<%=item.regCode%>');">刪除</button>
              </td>
            </tr>
            <% });
             } %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
