<script>
$(document).ready(function() {
	$(window).resize(function() { 
        if ($(this).width() > 800){
        	$(".typeside").animate({left:0},300);        	
        }else {
        	$(".typeside").animate({left:-240},300);
        	$(".viewside").css('margin-left','0px');
        }            
    });
}); 

</script>