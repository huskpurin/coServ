@import "wonderful.scss";
@import "scrollbar.scss";

.h4 {
	width: $w * 0.2;
	@include float;
}
.table {	
	width: $w * 0.35;	
	@include float;
	th {
		background: #c8c0b3;
		@include textC;
		padding: 5px;
	}
	td {
		border-bottom: 1px #ddd solid;
		vertical-align: middle;	
		padding: 5px;		
	}				
	td:first-child {
		@include textC;						
	}
}
.clear {
	@include clear;
}
.maR {
	margin-right: 50px;
}