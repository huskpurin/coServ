@import "wonderful.scss";

@mixin subjectInner{
	@include float;
	-webkit-padding-start: 25px;
	li {	
	line-height: 24px;
	font-size: $fs1;
	}
}

.wc27 {
	width: $w *0.27;
	@include subjectInner;	
}
.wc32 {
	width: $w *0.32;
	@include subjectInner;
	
}
.va {
	width: $w *0.43;
	@include subjectInner;
	vertical-align: bottom;
	margin-bottom: 0px;
	margin-left: -20px;
	margin-top: 170px;
}
.clear {
	@include clear;
}