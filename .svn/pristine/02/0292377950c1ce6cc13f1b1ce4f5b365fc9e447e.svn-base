@import "wonderful.scss";

.h4 {
	width: $w * 0.2;
	@include float;	
}
.table {	
	width: $w * 0.8;
	@include float;
	th {
		background: #c8c0b3;
		@include textC;
		padding: 5px;
	}
	td {
		@include td;		
	}
	td:first-child {
		@include textC;
	}
	td:last-child {
		border-right: none;
		@include textC;
	}
}
.clear {
	@include clear;
}
#nobor {
	border-bottom: none;
	padding-top: 20px;
}
