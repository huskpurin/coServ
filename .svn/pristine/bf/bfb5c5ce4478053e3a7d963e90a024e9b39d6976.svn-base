@import "wonderful.scss";

.modal-header {
  color: #fff;
  background-color: $fc4;
  border-color: #eea236;
  padding: 5px;
  @include fwY;
}
.modal-title {
	letter-spacing: $fs1 / 2;
}
.modal-body {
	padding: 20px 70px;
	color: $fc1;
	font-size: $fs1 * 1.1;	
	line-height: 1.3em;
	font-weight: 600;
	p{
		width: $w * 0.65;
		margin: 10px auto;
	}
	.text-indent{
		text-indent: 2em;
	}
	.spIDnInfo{
		width: $w * 0.8;
		margin: 10px auto;
		text-align: center;
		.oline{
			color: $fc4;
		}
	}
	.attendeeInfo{
		width: $w * 0.4 ;
		margin: 5px auto;
	}
}
.modal-footer {
	border-top: none;
	padding-top: 0px;
	a{
		margin: 0 10px;
	}
}

.clear {
	@include clear;
}
