@import "wonderful.scss";

.repager {	
	position: relative;	
	.title{	
		@include title;
	}
	.article {
		@include article;

		.section{
			@include section;
			h4{
				width: 155px;
			}
			.table {
				float: right;
				width: 715px;
				margin-top: -30px;
				th {
					background: #c8c0b3;
					text-align: center;
				}				
				td {
					border-bottom: 1px #eee solid;
					border-left: 1px #eee solid;					
				}
			}
			#regForm {
				float: right;
				width: 715px;
				margin-top: -30px;
			}
		}
	}
}

tr:nth-child(5),
tr:nth-child(10) {
	background: #eaded0;
}
.borderB {
	border-bottom: 1px #ccc solid;
}
.boline {  
  background-color: transparent;
  border: none !important;
  border-bottom: 1px solid #cccccc;
  -webkit-box-shadow: none;
  box-shadow: 0;
}
.maT {
	margin-top: 50px;
}
.clear {
	@include clear;
}
.pa{
	padding-left: 0;
	padding-right: 0;
}
#nobor {
	border-bottom: none;
}

.modal-dialog {
	width: 1024px;
}
