ctrl.startup = function() {
	ctrl.embed('.viewside', '/coserv/docList', {params:{"_loc":"<%=bi.locale%>"}});

	$( window ).scroll(function() {
	    var  y = window.pageYOffset;
	    if (y){
	    	ctrl.sel(".gotop").fadeIn(300);
	    }
	    else {
	    	ctrl.sel(".gotop").fadeOut(300);
	    }
	});	 

	if (ctrl.detectmob()) {
		ctrl.sel(".article").addClass('inner');
		ctrl.sel("pre").addClass('preChang');
		ctrl.sel(".gotop").addClass('gotopChang');
	}
	else {
		ctrl.sel(".article").removeClass('inner');
		ctrl.sel("pre").removeClass('preChang');
		ctrl.sel(".gotop").removeClass('gotopChang');
	}
};

ctrl.detectmob = function () {
	var  isMob = false;
	if( navigator.userAgent.match(/Android/i) || navigator.userAgent.match(/webOS/i)
			|| navigator.userAgent.match(/iPhone/i)	|| navigator.userAgent.match(/iPad/i)
			|| navigator.userAgent.match(/iPod/i) || navigator.userAgent.match(/BlackBerry/i)
			|| navigator.userAgent.match(/Windows Phone/i)
	){
		isMob = true;
	}
	return isMob;
}