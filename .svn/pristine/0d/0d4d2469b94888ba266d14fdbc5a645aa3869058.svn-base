@import "wonderful.scss";

.modal-header {
  color: #fff;
  background: $fc5;
  // border-color: #eea236;
  padding: 5px;
  @include fwY;
}
// .modal-title {
// 	letter-spacing: $fs1 / 2;
// }
.modal-body {
	padding: 10px 70px;
	margin-top: 15px;
	color: $fc1;
	font-size: $fs1 * 0.9;	
	line-height: $fs1 * 2;
	font-weight: 600;
	margin-bottom:10px;
	
	p{
		width: $w * 0.65;
		margin: 10px auto;
	}
	// .text-indent{
	// 	text-indent: 2em;
	// }
	.spIDnInfo{
		width: -moz-fit-content;
		width: -webkit-fit-content;
   		width: fit-content;
    	margin: auto;
    	margin: 10px auto;		
    	text-align: center;
   		padding: 1em 0;
		border-top: 1px solid $f4;
		border-bottom: 1px solid $f4;
		.badge{
			background: $fc4 !important;
		}
		.oline{
			color: $fc4;
		}
	}
	.attendeeInfo{
		width: $w * 0.4 ;
		margin: 5px auto;
	}
}
.modal-footer {
	border-top: none;
	padding-top: 0px;
	a{
		margin: 0 10px;
	}
}

.clear {
	@include clear;
}