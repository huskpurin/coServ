<div class="col-md-offset-3 col-md-6 text-center">
  <h3>後台登入</h3>
  <form id="login" role="form">
      <div class="form-group">
        <input type="text" id="accName" name="accName" class="form-control text-center" value="" placeholder="帳號" onkeydown="if(window.event.keyCode==13)<%=ctrl%>.focusPw();">
      </div>
      <div class="form-group">
        <input type="password" id="passwd" name="passwd" class="form-control text-center" value="" placeholder="密碼" onkeydown="if(window.event.keyCode==13)<%=ctrl%>.doLogin();">
      </div>
      <div class="form-group">
        <button type="button" class="form-control" onclick="<%=ctrl%>.doLogin();">登入</button>
      </div>
  </form>
</div>
