@import "wonderful.scss";

.modal-header {
  color: #fff;
  background-color: #f0ad4e;
  border-color: #eea236;
}

.repager {	
	position: relative;	
	.title{	
		@include title;
	}
	article {
		@include article;

		section{
			@include section;
			h4{
				width: 155px;
			}
			.table {
				float: right;
				width: 715px;
				margin-top: -30px;
				th {
					background: #c8c0b3;
					text-align: center;
				}				
				td {
					border-bottom: 1px #eee solid;
					border-left: 1px #eee solid;					
				}
			}
			#regForm {
				float: right;
				width: 715px;
				margin-top: -30px;
			}
		}
	}
}
.clear {
	clear: both;
}

tr:nth-child(5),
tr:nth-child(10) {
	background: #eaded0;
}
.nobor {
	border-bottom: 0;
}
