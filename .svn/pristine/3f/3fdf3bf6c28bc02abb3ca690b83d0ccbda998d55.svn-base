@import "wonderful.scss";

.modal-header {
  color: #fff;
  background-color: $fc4;
  border-color: #eea236;
  padding: 5px;
  @include fwY;
}
.modal-title {
	letter-spacing: $fs1 / 2;
}
.modal-body {
	padding: 20px 30px;
	color: $fc1;
	font-size: $fs1;
	line-height: 1.3em;
	font-weight: 600;
	margin-bottom:20px;
	p{
		width: $w * 0.8;
	    width: -moz-fit-content;
	    width: -webkit-fit-content;
	    width: fit-content;
	}
	.text-indent{
		text-indent: 2em;
	}
	.text-center {
		margin: 0 auto;
	}
	.spIDnInfo{
	    width: -moz-fit-content;
	    width: -webkit-fit-content;
	    width: fit-content;
	    margin: auto;
	    margin: 10px auto;
			text-align: center;
	    padding: 1em 0;
	    border-top: 1px solid $fc4;
	    border-bottom: 1px solid $fc4;
			.oline{
				color: $fc4;
			}
		}
		.attendeeInfo{
			width: $w * 0.4;
			margin: 5px auto;
			p{
			text-align: left;
			}

	    .fieldName {
	      color: $fc4;
	      margin-right: 20px;
    	}
	}
}

.badge {
  font-size: 20px!important;
  background: $fc4;
  min-width: 15px;
  border-radius: 15px;
}
.modal-footer {
	border-top: none;
	padding-top: 0px;
	a{
		margin: 0 10px;
	}
}

.clear {
	@include clear;
}
